extend layout
block content
    .page-header
      h1 Wahlkreissieger

    .main-results
      table.table.table-striped
        thead
          tr
            th Nr.
            th Wahlkreis
            th Siegerpartei Erststimmen
            th
            th Siegerpartei Zweitstimmen
            th
        tbody
          each value, index in resTable
            tr
              th= value.nummer
              th= value.wahlkreis
              th= value.winnerpartyerststimmen
              th(style= "width:30px; background-color:"+ value.erstcolor)
              th= value.winnerpartyzweitstimmen
              th(style= "width:30px; background-color:"+ value.zweitcolor)
            
          
