extend layout    
block content
    .page-header
      h1 Wahlkreis√ºbersicht

    .main-results
      select(id="wahlkreise")
      //each value, index in resTable
        each value, index in resTable1
          option(value= value.wahlkreis)= value.wahlkreis


      table.table.table-striped
        thead
          tr
            th Nr.
            th Wahlkreis
            th Wahlbeteiligung
            th Siegerkandidat
        tbody
        //each value, index in resTable1
          each value, index in resTable
            tr
              th= value.nummer
              th= value.wahlkreis
              th= ((value.wahlbeteiligung)*100).toFixed(2) + "%"
              th= value.candidate

           
