extend layout
block content
  .page-header
    h1 Mitglieder des Bundestags
    
  .main-results 
    table.table.table-striped
      thead
        tr
          th Nr.
          th Kandidat
          th Partei
          th
          th Bundesland
      tbody
          each value, index in resTable
            tr
              th= index+1
              th= value.candidate
              th= value.party
              th(style= "width:30px; background-color:"+ value.color)
              th= value.federalland
